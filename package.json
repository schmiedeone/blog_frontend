{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo-boost": "^0.4.9",
    "cypress": "^4.10.0",
    "graphql": "^15.3.0",
    "material-ui": "^0.20.2",
    "moment": "^2.27.0",
    "range-inclusive": "^1.0.2",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-moment": "^0.9.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "predeploy": "rm -rf ./build/ && yarn build",
    "clean": "aws s3 rm --recursive s3://schmiede-blog",
    "deploy": "aws s3 cp ./build/ s3://schmiede-blog --recursive --acl public-read",
    "clean:deploy": "yarn clean && yarn deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
